{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "pretty": true,
    "target": "es6",
    "experimentalDecorators": true,
"emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist"
  },
  "include": ["lib/**/*.ts"],
  "exclude": ["node_modules"],
  "lib": ["es2015"]
}
